<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Bean.Location" table="location">
  
      <id name="locationId" column="LocationId" type="string"></id>
      <property name="locationName" column="LocationName" type="string" />
      <property name="locationAddress" column="Address" type="string" />
  </class>
</hibernate-mapping>
